<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://service.wellsfargo.com/provider/fxaip/gfx/service/core/2013/" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msg="http://service.wellsfargo.com/entity/message/2007/" xmlns:ss="http://service.wellsfargo.com/provider/fxaip/gfx/service/settlement/2013/" xmlns:ts="http://service.wellsfargo.com/provider/fxaip/gfx/service/trading/2013/" xmlns:tr="http://service.wellsfargo.com/provider/fxaip/gfx/trading/2013/" xmlns:fxbase="http://service.wellsfargo.com/provider/fxaip/base/2013/" targetNamespace="http://service.wellsfargo.com/provider/fxaip/gfx/service/core/2013/" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="1.01">
	<xs:import namespace="http://service.wellsfargo.com/entity/message/2007/" schemaLocation="./../../../../service/entity/message/message2007.xsd"/>
	<xs:import namespace="http://service.wellsfargo.com/provider/fxaip/gfx/service/settlement/2013/" schemaLocation="./settlementService2013.xsd"/>
	<xs:import namespace="http://service.wellsfargo.com/provider/fxaip/gfx/service/trading/2013/" schemaLocation="./tradingService2013.xsd"/>
	<xs:import namespace="http://service.wellsfargo.com/provider/fxaip/gfx/trading/2013/" schemaLocation="./trading/trading2013.xsd"/>
	<xs:import namespace="http://service.wellsfargo.com/provider/fxaip/base/2013/" schemaLocation="./../base/fxaip-base2013.xsd"/>
	
	<!-- -->
	<!-- Copyright 2013 Wells Fargo Bank. All rights reserved.-->
	<!-- -->
	<!-- BEGIN CHANGE HISTORY-->
	<!-- 2013.01.03-1.01 Updated namespaces and references (J.Celauro) -->
	<!-- 2013.01.02-1.00 Initial revision (J.Celauro)-->
	<!-- END CHANGE HISTORY-->
	<!-- -->
	<xs:annotation>
		<xs:documentation>Core FX services will be defined (types) in their respective xsd files (e.g. settlementService2013.xsd).
			Aggregate service methods (those that make use of one or more coreFX services or types) will be defined here.
				
		</xs:documentation>
	</xs:annotation>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Trading Request-Response   				 -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:element name="requestDeal" type="ts:RequestDeal_Type"/>
	<xs:element name="requestDealResponse" type="ts:RequestDealResponse_Type"/>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Settlement Center Request-Response  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:element name="submitSettlement" type="ss:SubmitSettlement_Type"/>
	<xs:element name="submitSettlementResponse" type="ss:SubmitSettlementResponse_Type"/>
	<!-- ############################################################################################ -->
	<!--  Aggregate Method Type Definitions         																										-->
	<!-- ############################################################################################ -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  RequestDealAndSettlement Types        -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:complexType name="RequestDealAndSettlement_Type">
		<xs:annotation>
			<xs:documentation>Aggregate method for requesting a Deal and submitting Settlement Instructions</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="requestDeal"/>
			<xs:element ref="submitSettlement"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RequestDealAndSettlementResponse_Type">
		<xs:annotation>
			<xs:documentation>Response message for RequestDealAndSettlement request</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="msg:WFFaultList" minOccurs="0"/>
			<xs:element ref="tr:TradeDetail" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- Request Deal And Settlement Request-Response -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:element name="requestDealAndSettlement" type="RequestDealAndSettlement_Type"/>
	<xs:element name="requestDealAndSettlementResponse" type="RequestDealAndSettlementResponse_Type"/>
</xs:schema>
