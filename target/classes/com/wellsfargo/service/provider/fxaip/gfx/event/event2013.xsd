<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://service.wellsfargo.com/provider/fxaip/gfx/event/2013/"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:fxbase="http://service.wellsfargo.com/provider/fxaip/base/2013/"
           xmlns:wfbase="http://service.wellsfargo.com/base/2003/"
           xmlns:trading="http://service.wellsfargo.com/provider/fxaip/gfx/trading/2013/"
           targetNamespace="http://service.wellsfargo.com/provider/fxaip/gfx/event/2013/"
           elementFormDefault="unqualified" attributeFormDefault="unqualified" version="1.00">

    <xs:import namespace="http://service.wellsfargo.com/provider/fxaip/base/2013/"
               schemaLocation="./../../base/fxaip-base2013.xsd"/>
    <xs:import namespace="http://service.wellsfargo.com/provider/fxaip/gfx/trading/2013/"
               schemaLocation="../trading/trading2013.xsd"/>
    <xs:import namespace="http://service.wellsfargo.com/base/2003/" schemaLocation="../../../../base/base2003.xsd"/>
    <!-- -->
    <!-- Copyright 2013 Wells Fargo Bank. All rights reserved.-->
    <!-- -->
    <!-- BEGIN CHANGE HISTORY-->
    <!-- 2013.02.22-1.00 Initial revision (C. Jansen)-->
    <!-- END CHANGE HISTORY-->
    <!-- -->
    <!-- -->
    <xs:annotation>
        <xs:documentation>Components related to GFX events</xs:documentation>
    </xs:annotation>
    <!-- This should be exposed in the Service schema -->
    <xs:complexType name="AFXTradeRequest_Type">
        <xs:annotation>
            <xs:documentation>Message between FXAIP and AFX. The intent is to send minimal data to help ensure
                transmission speed
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="baseCurrencyAmount" type="xs:decimal" minOccurs="0"/>
            <xs:element name="baseSpotRate" type="trading:SpotFwdAllInRate_Type"/>
            <xs:element name="currency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
            <xs:element name="currencyAmount" type="xs:decimal" minOccurs="0"/>
            <xs:element name="ctrCurrency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
            <xs:element name="ctrCurrencyAmount" type="xs:decimal" minOccurs="0"/>
            <xs:element name="sourceReferenceId" type="fxbase:ReferenceId_Type" minOccurs="0"/>
            <xs:element name="sourceSystem" type="fxbase:SourceSystem_Type" minOccurs="0"/>
            <xs:element name="status" type="xs:string" minOccurs="0"/>
            <xs:element name="tradeId" type="xs:long" minOccurs="0"/>
            <xs:element name="updateUser" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="PRM_SpotFwd_WindowFwd_TradeRequest_Type">
        <xs:annotation>
            <xs:documentation>Message between FXAIP and PRM for spot fwd/window fwds. The intent is to send minimal data
                to help ensure transmission speed
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="PRM_PositionTransfer_Swap_TradeRequest_Type">
                <xs:sequence>
                    <xs:element name="afxStatus" type="xs:string" minOccurs="0"/>
                    <xs:element name="allInSpread" type="xs:decimal" minOccurs="0"/>
                    <xs:element name="baseAllInRate" type="trading:SpotFwdAllInRate_Type" minOccurs="0"/>
                    <xs:element name="baseCurrencyAmount" type="xs:decimal" minOccurs="0"/>
                    <xs:element name="baseCurrency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
                    <xs:element name="baseCurrencyAmountTerm" type="fxbase:CurrencyTerm_Enum" minOccurs="0"/>
                    <xs:element name="baseFwdPoints" type="xs:decimal" minOccurs="0"/>
                    <xs:element name="baseSpotRate" type="trading:SpotFwdAllInRate_Type" minOccurs="0"/>
                    <xs:element name="baseOutright" type="xs:decimal" minOccurs="0"/>
                    <xs:element name="counterpartyId" type="xs:decimal" minOccurs="0"/>
                    <xs:element name="currency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
                    <xs:element name="currencyAmount" type="wfbase:CurrencyAmt_Type" minOccurs="0"/>
                    <xs:element name="currencyValueDate" type="xs:date" minOccurs="0"/>
                    <xs:element name="ctrCurrency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
                    <xs:element name="ctrCurrencyAmount" type="wfbase:CurrencyAmt_Type" minOccurs="0"/>
                    <xs:element name="customerPoints" type="trading:SpotFwdAllInRate_Type" minOccurs="0"/>
                    <xs:element name="eventSource" type="xs:string" minOccurs="0"/>
                    <xs:element name="fixingDate" type="xs:date" minOccurs="0"/>
                    <xs:element name="fromPortfolio" type="xs:string" minOccurs="0"/>
                    <xs:element name="fromTrader" type="xs:string" minOccurs="0"/>
                    <xs:element name="inverse" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="marketFwdPoints" type="xs:decimal" minOccurs="0"/>
                    <xs:element name="marketSpotRate" type="xs:decimal" minOccurs="0"/>
                    <xs:element name="optionDate" type="xs:date" minOccurs="0"/>
                    <xs:element name="optionId" type="xs:integer" minOccurs="0"/>
                    <xs:element name="originalCcyAmount" type="wfbase:CurrencyAmt_Type" minOccurs="0"/>
                    <xs:element name="originalCtrCcyAmount" type="wfbase:CurrencyAmt_Type" minOccurs="0"/>
                    <xs:element name="sourceReferenceId" type="fxbase:ReferenceId_Type" minOccurs="0"/>
                    <xs:element name="sourceSystem" type="fxbase:SourceSystem_Type" minOccurs="0"/>
                    <xs:element name="spotSpread" type="xs:decimal" minOccurs="0"/>
                    <xs:element name="status" type="xs:string" minOccurs="0"/>
                    <xs:element name="windowStartDate" type="xs:decimal" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="PRM_PositionTransfer_Swap_TradeRequest_Type">
        <xs:annotation>
            <xs:documentation>Message between FXAIP and PRM for position transfers.
                The intent is to send minimal data to help ensure transmission speed
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="backOfficeId" type="xs:string" minOccurs="0"/>
            <xs:element name="backOfficeSeq" type="xs:string" minOccurs="0"/>
            <xs:element name="baseCurrency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
            <xs:element name="baseSpotRate" type="trading:SpotFwdAllInRate_Type" minOccurs="0"/>
            <xs:element name="buySellIndicator" type="fxbase:BuySellIndicator_Enum" minOccurs="0"/>
            <xs:element name="branch" type="xs:string"/>
            <xs:element name="currency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
            <xs:element name="ctrCurrency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
            <xs:element name="currencyAmount" type="wfbase:CurrencyAmt_Type" minOccurs="0"/>
            <xs:element name="ctrCurrencyAmount" type="wfbase:CurrencyAmt_Type" minOccurs="0"/>
            <xs:element name="currencyTerm" type="fxbase:CurrencyTerm_Enum" minOccurs="0"/>
            <xs:element name="dealDate" type="xs:date"/>
            <xs:element name="operator" type="xs:string"/>
            <xs:element name="productId" type="xs:string" minOccurs="0"/>
            <xs:element name="status" type="xs:string" minOccurs="0"/>
            <xs:element name="sourceReferenceId" type="fxbase:ReferenceId_Type" minOccurs="0"/>
            <xs:element name="sourceSystem" type="fxbase:SourceSystem_Type" minOccurs="0"/>
            <xs:element name="traderBooks" type="trading:FXTraderBooks_Type"/>
            <xs:element name="tradeId" type="xs:integer" minOccurs="0"/>
            <xs:element name="valueDate" type="fxbase:TenorDate_Type"/>
            <xs:element name="updateTime" type="xs:date" minOccurs="0"/>
            <xs:element name="updateUser" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="PRM_SwapLeg_TradeRequest_Type">
        <xs:annotation>
            <xs:documentation>Message between FXAIP and PRM for swap legs. The intent is to send minimal data to help ensure
                transmission speed
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="farAllInSpread" type="xs:decimal" minOccurs="0"/>
            <xs:element name="farAllInSpreadAmount" type="wfbase:CurrencyAmt_Type" minOccurs="0"/>
            <xs:element name="farBackOfficeId" type="xs:long" minOccurs="0"/>
            <xs:element name="farBaseCurrencyAmount" type="wfbase:CurrencyAmt_Type" minOccurs="0"/>
            <xs:element name="farBaseCurrencyTerm" type="fxbase:CurrencyTerm_Enum" minOccurs="0"/>
            <xs:element name="farBaseRateMarketConvention" type="xs:boolean" minOccurs="0"/>
            <xs:element name="farBaseSpotRate" type="xs:decimal" minOccurs="0"/>
            <xs:element name="farBuySellIndicator" type="fxbase:BuySellIndicator_Enum" minOccurs="0"/>
            <xs:element name="farCtrCurrencyAmount" type="xs:decimal" minOccurs="0"/>
            <xs:element name="farCurrencyAmount" type="wfbase:CurrencyAmt_Type" minOccurs="0"/>
            <xs:element name="farCurrencyTerm" type="fxbase:CurrencyTerm_Enum" minOccurs="0"/>
            <xs:element name="farCustomerAllInRate" type="xs:decimal" minOccurs="0"/>
            <xs:element name="farCustomerFwdPoints" type="xs:decimal" minOccurs="0"/>
            <xs:element name="farCustomerSpotRate" type="xs:decimal" minOccurs="0"/>
            <xs:element name="farMarketAllInRate" type="xs:decimal" minOccurs="0"/>
            <xs:element name="farMarketFwdPoints" type="xs:decimal" minOccurs="0"/>
            <xs:element name="farMarketSpotRate" type="xs:decimal" minOccurs="0"/>
            <xs:element name="farValueDate" type="xs:date" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>
