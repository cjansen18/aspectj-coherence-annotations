<xs:schema xmlns="http://service.wellsfargo.com/provider/fxaip/gfx/settlement/2013/" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:fxbase="http://service.wellsfargo.com/provider/fxaip/base/2013/" xmlns:wfbase="http://service.wellsfargo.com/base/2003/" targetNamespace="http://service.wellsfargo.com/provider/fxaip/gfx/settlement/2013/" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="1.02">
	<xs:import namespace="http://service.wellsfargo.com/provider/fxaip/base/2013/" schemaLocation="./../../base/fxaip-base2013.xsd"/>
	<xs:import namespace="http://service.wellsfargo.com/base/2003/" schemaLocation="../../../../base/base2003.xsd"/>
