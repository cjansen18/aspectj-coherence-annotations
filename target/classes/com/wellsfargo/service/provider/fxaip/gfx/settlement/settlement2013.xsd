	<xs:annotation>
		<xs:documentation>Components for managing Settlement Instructions. </xs:documentation>
	</xs:annotation>
	<xs:simpleType name="WireType_Enum">
		<xs:annotation>
			<xs:documentation>Different type of Wire payments for the SettlementWire_Type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="BANKWIRE"/>
			<xs:enumeration value="CORPWIRE"/>
			<xs:enumeration value="FEDWIRE"/>
			<xs:enumeration value="DRAFT"/>
			<xs:enumeration value="NOSTRO"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MCAAccountType_Enum">
		<xs:annotation>
			<xs:documentation/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="CAYMAN"/>
			<xs:enumeration value="LONDON"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DetailOfCharges_Enum">
		<xs:annotation>
			<xs:documentation>TODO</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="BENE"/>
			<xs:enumeration value="SHA"/>
			<xs:enumeration value="OUR"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ReferenceField_Type">
		<xs:annotation>
			<xs:documentation>Optional user supplied reference field (50 characters max)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="InstructionDescription_Type">
		<xs:annotation>
			<xs:documentation>Optional description</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="150"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="SettlementInstructionDetail_Type">
		<xs:annotation>
			<xs:documentation>TODO</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="accountNumber" type="xs:string" minOccurs="0"/>
			<xs:element name="bic" type="xs:string" minOccurs="0"/>
			<xs:element name="clearingSystemCode" type="xs:string" minOccurs="0"/>
			<xs:element name="settlementText1" type="xs:string" minOccurs="0"/>
			<xs:element name="settlementText2" type="xs:string" minOccurs="0"/>
			<xs:element name="settlementText3" type="xs:string" minOccurs="0"/>
			<xs:element name="settlementText4" type="xs:string" minOccurs="0"/>
			<xs:element name="settlementText5" type="xs:string" minOccurs="0"/>
			<xs:element name="settlementText6" type="xs:string" minOccurs="0"/>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="SettlementDetails_Type">
		<xs:annotation>
			<xs:documentation>TODO</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="accountWithInstitution" type="SettlementInstructionDetail_Type" minOccurs="0"/>
			<xs:element name="beneficiary" type="SettlementInstructionDetail_Type" minOccurs="0"/>
			<xs:element name="intermediaryInstitution" type="SettlementInstructionDetail_Type" minOccurs="0"/>
			<xs:element name="orderingBank" type="SettlementInstructionDetail_Type" minOccurs="0"/>
			<xs:element name="orderingCustomer" type="SettlementInstructionDetail_Type" minOccurs="0"/>
			<xs:element name="senderToReceiver" type="SettlementInstructionDetail_Type" minOccurs="0"/>
			<xs:element name="paymentDetails" type="SettlementInstructionDetail_Type" minOccurs="0"/>
			<xs:element name="receiverDetails" type="SettlementInstructionDetail_Type" minOccurs="0"/>
			<xs:element name="receiverCorrespondent" type="SettlementInstructionDetail_Type" minOccurs="0"/>
			<xs:element name="sendersCorrespondent" type="SettlementInstructionDetail_Type" minOccurs="0"/>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="SettlementHeader_Type" abstract="true">
		<xs:annotation>
			<xs:documentation>TODO</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="reference1" type="ReferenceField_Type" minOccurs="0"/>
			<xs:element name="reference2" type="ReferenceField_Type" minOccurs="0"/>
			<xs:element name="reference3" type="ReferenceField_Type" minOccurs="0"/>
			<xs:element name="description" type="InstructionDescription_Type" minOccurs="0"/>
			<xs:element name="detailOfCharges" type="DetailOfCharges_Enum" minOccurs="0"/>
			<!--
			<xs:element name="settlementMethod" type="SettlementMethod_Enum"/>
			<xs:element name="settlementAccount" type="xs:string" minOccurs="0"/>
			
			<xs:element name="baiCode" type="xs:string" minOccurs="0"/>
			-->
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SettlementDDA_Type">
		<xs:annotation>
			<xs:documentation>Represents settling via DDA account</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SettlementHeader_Type">
				<xs:sequence>
					<xs:element name="accountNumber" type="xs:string"/>
					<xs:element name="routingNumber" type="xs:string"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SettlementGL_Type">
		<xs:annotation>
			<xs:documentation>Represents settling via GL</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SettlementHeader_Type">
				<xs:sequence>
					<xs:element name="settlementMeans" type="xs:string"/>
					<xs:element name="settlementAccount" type="xs:string"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SettlementWire_Type">
		<xs:annotation>
			<xs:documentation>Represents settling via a Wire. See the WireType_Enum for the different wire types</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SettlementHeader_Type">
				<xs:sequence>
					<xs:element name="nostro" type="xs:string" minOccurs="0"/>
					<xs:element name="type" type="WireType_Enum" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SettlementMCA_Type">
		<xs:annotation>
			<xs:documentation>TODO</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SettlementHeader_Type">
				<xs:sequence>
					<xs:element name="accountNumber" type="xs:string"/>
					<xs:element name="accountType" type="MCAAccountType_Enum" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SettlementIIS_Type">
		<xs:annotation>
			<xs:documentation>TODO</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SettlementHeader_Type">
				<xs:sequence>
					<xs:element name="accountNumber" type="xs:string"/>
					<xs:element name="routingNumber" type="xs:string"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SettlementDACH_Type">
		<xs:annotation>
			<xs:documentation>TODO</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SettlementHeader_Type">
				<xs:sequence>
					<xs:element name="accountNumber" type="xs:string"/>
					<xs:element name="routingNumber" type="xs:string"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SettlementInfo_Type">
		<xs:annotation>
			<xs:documentation>Validation Information</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="currency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
			<xs:element name="amount" type="xs:decimal" minOccurs="0"/>
		</xs:all>
	</xs:complexType>
	<xs:element name="settlementHeader" type="SettlementHeader_Type" abstract="true"/>
	<xs:element name="DDA-Settlement" type="SettlementDDA_Type" substitutionGroup="settlementHeader"/>
	<xs:element name="GL-Settlement" type="SettlementGL_Type" substitutionGroup="settlementHeader"/>
	<xs:element name="Wire-Settlement" type="SettlementWire_Type" substitutionGroup="settlementHeader"/>
	<xs:element name="MCA-Settlement" type="SettlementMCA_Type" substitutionGroup="settlementHeader"/>
	<xs:element name="DACH-Settlement" type="SettlementDACH_Type" substitutionGroup="settlementHeader"/>
	<xs:element name="IIS-Settlement" type="SettlementIIS_Type" substitutionGroup="settlementHeader"/>
	<xs:complexType name="SettlementInstruction_Type">
		<xs:annotation>
			<xs:documentation>Represents WellsFargo Pay Instruction (outgoing instruction) Or Receive Instruction (incoming instruction)</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="paymentInfo" type="SettlementInfo_Type" minOccurs="0"/>
			<xs:element ref="settlementHeader"/>
			<xs:element name="settlementDetails" type="SettlementDetails_Type" minOccurs="0"/>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="SettlementData_Type" abstract="true">
		<xs:annotation>
			<xs:documentation>Base type for Settlement Data and Instructions</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="source" type="xs:string" minOccurs="0"/>
			<xs:element name="dealIdentifier" type="fxbase:TradeContractId_Type" minOccurs="0"/>
			<xs:element name="updateUser" type="fxbase:Entity_Type" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SingleSettlementData_Type">
		<xs:annotation>
			<xs:documentation>Represents either the incoming or outgoing payment flow (instructions) for settlement</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SettlementData_Type">
				<xs:sequence>
					<xs:choice>
						<xs:element name="wfPayInstruction" type="SettlementInstruction_Type"/>
						<xs:element name="wfReceiveInstruction" type="SettlementInstruction_Type"/>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="BasicSettlementData_Type">
		<xs:annotation>
			<xs:documentation>Represents one incoming and one outgoing payment flow (instructions) for settlement</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SettlementData_Type">
				<xs:sequence>
					<xs:element name="wfPayInstruction" type="SettlementInstruction_Type"/>
					<xs:element name="wfReceiveInstruction" type="SettlementInstruction_Type"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SwapSettlementData_Type">
		<xs:annotation>
			<xs:documentation>Represents incoming/outgoing payment flows for each leg of a Swap Deal</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SettlementData_Type">
				<xs:sequence>
					<xs:element name="wfNearLegPayInstruction" type="SettlementInstruction_Type"/>
					<xs:element name="wfNearLegReceiveInstruction" type="SettlementInstruction_Type"/>
					<xs:element name="wfFarLegPayInstruction" type="SettlementInstruction_Type"/>
					<xs:element name="wfFarLegReceiveInstruction" type="SettlementInstruction_Type"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SettlementStatusList_Type">
		<xs:annotation>
			<xs:documentation>A list of Settlement Status</xs:documentation>
		</xs:annotation>
		<xs:all>
			<!--<xs:element ref="egi:TradeDetail" maxOccurs="unbounded"/>-->
		</xs:all>
	</xs:complexType>
	<xs:element name="SettlementData" type="SettlementData_Type" abstract="true"/>
	<xs:element name="SingleSettlementData" type="SingleSettlementData_Type" substitutionGroup="SettlementData"/>
	<xs:element name="BasicSettlementData" type="BasicSettlementData_Type" substitutionGroup="SettlementData"/>
	<xs:element name="SwapSettlementData" type="SwapSettlementData_Type" substitutionGroup="SettlementData"/>
</xs:schema>
