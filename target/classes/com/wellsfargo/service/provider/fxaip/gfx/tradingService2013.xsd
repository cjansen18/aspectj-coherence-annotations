	<xs:annotation>
		<xs:documentation>Trade Processing Request/Response Message Schema. Available operations are:
									  - Request Deal (Trade Request)
		</xs:documentation>
	</xs:annotation>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Request Deal Request-Response  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:complexType name="RequestDeal_Type">
		<xs:annotation>
			<xs:documentation>Book one or more trades in the system</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="tr:TradeRequest"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="RequestDealResponse_Type">
		<xs:annotation>
			<xs:documentation>Response for RequestDeal request</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="msg:WFFaultList" minOccurs="0"/>
			<xs:element ref="tr:TradeDetailList" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
