	<xs:annotation>
		<xs:documentation>Settlement Center Request/Response Message Schema. Available operations are:
									  - Approve Settlement
									  - Cancel Settlement
									  - Get Settlement
									  - Reject Settlement
									  - Remove Settlement
									  - Save Settlement
									  - Submit Settlement
		</xs:documentation>
	</xs:annotation>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Approve Settlement Request-Response  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:element name="ApproveSettlement_Type">
		<xs:annotation>
			<xs:documentation>Approve Instructions for one or more trades that have a status of Pending Authorization</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="approver" type="fxbase:Entity_Type"/>
				<xs:element name="tradeContractIdList" type="fxbase:TradeContractIdList_Type" nillable="false"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ApproveSettlementResponse_Type">
		<xs:annotation>
			<xs:documentation>Response to ApproveSettlement message</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="msg:WFFaultList" minOccurs="0"/>
				<xs:element name="settlementStatusList" type="sc:SettlementStatusList_Type" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
