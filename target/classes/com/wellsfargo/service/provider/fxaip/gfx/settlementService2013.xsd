	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->	
	<xs:complexType name="SubmitSettlement_Type">
		<xs:annotation>
			<xs:documentation>Request type for submitting Settlement Instructions. 	 
			If number of approvers greater than zero, will have Pending Authorization status; otherwise will be in Authorized status</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="sc:SettlementData"/>
			<xs:element name="numberOfApprovers" type="xs:int" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SubmitSettlementResponse_Type">
		<xs:annotation>
