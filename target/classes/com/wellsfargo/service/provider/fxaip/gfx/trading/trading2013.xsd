				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AmendMCATradeRequest_Type">
		<xs:complexContent>
			<xs:extension base="AmendTradeRequest_Type">
				<xs:sequence>
					<xs:element name="dealDate" type="xs:date" minOccurs="0"/>
					<xs:element name="currencyAmount" type="wfbase:CurrencyAmt_Type" minOccurs="0"/>
					<xs:element name="coBankFees" type="xs:decimal" minOccurs="0"/>
					<xs:element name="coBankName" type="xs:string" minOccurs="0"/>
					<xs:element name="creditAccount" type="xs:string" minOccurs="0"/>
					<xs:element name="debitAccount" type="xs:string" minOccurs="0"/>
					<xs:element name="deliveryInstructions" type="xs:int" minOccurs="0"/>
					<xs:element name="mcaType" type="McaType_Enum" minOccurs="0"/>
					<xs:element name="specialInstructions" type="xs:string" minOccurs="0"/>
					<xs:element name="valueDate" type="fxbase:TenorDate_Type" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	-->
	<!-- ################################################### -->
	<!-- #           Trade Details Types 					    					 # -->
	<!-- ################################################### -->
	<xs:complexType name="Contract_Type" abstract="true">
		<xs:annotation>
			<xs:documentation>Base type for all trades - contains the contract details</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="tradeId" type="xs:long"/>
			<xs:element name="branch" type="xs:string"/>
			<xs:element name="contractId" type="xs:string"/>
			<xs:element name="contractSeq" type="xs:string"/>
			<xs:element name="status" type="xs:string"/>
			<xs:element name="inTime" type="xs:dateTime"/>
			<xs:element name="productId" type="fxbase:Product_Enum"/>
			<xs:element name="sourceSystem" type="fxbase:SourceSystem_Type"/>
			<xs:element name="externalRefId1" type="xs:string" minOccurs="0"/>
			<xs:element name="externalRefId2" type="xs:string" minOccurs="0"/>
			<xs:element name="revenueEntityId" type="fxbase:Entity_Type" minOccurs="0"/>
			<xs:element name="updateTime" type="xs:dateTime"/>
			<xs:element name="updateUser" type="xs:string"/>
			<xs:element name="flipExternalResponse" type="xs:boolean"/>
			<xs:element name="sourceReferenceId" type="fxbase:ReferenceId_Type" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SpotFwdContract_Type">
		<xs:annotation>
			<xs:documentation>Represents a Spot/Fwd Contract</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Contract_Type">
				<xs:sequence>
					<xs:element name="dealDate" type="xs:date"/>
					<!--
					<xs:element name="traderBooks" type="FXTraderBooks_Type"/>
					-->
					<xs:element name="traderBooks" type="TraderBooks_Type" minOccurs="0"/>
					<xs:element name="portfolios" type="Portfolios_Type" minOccurs="0"/>
					<xs:element name="ccyValueDate" type="fxbase:TenorDate_Type" minOccurs="0"/>
					<xs:element name="ctrValueDate" type="fxbase:TenorDate_Type" minOccurs="0"/>
					<xs:element name="originalCcyAmount" type="xs:decimal" minOccurs="0"/>
					<xs:element name="originalCtrAmount" type="xs:decimal" minOccurs="0"/>
					<xs:element name="details" type="SpotFwdLeg_Type"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="WindowFwdContract_Type">
		<xs:annotation>
			<xs:documentation>Represents a WindowFwd Contract</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SpotFwdContract_Type">
				<xs:sequence>
					<xs:element name="optionDate" type="fxbase:TenorDate_Type"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SwapContract_Type">
		<xs:annotation>
			<xs:documentation>Represents a Swap Contract</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Contract_Type">
				<xs:sequence>
					<xs:element name="dealDate" type="xs:date"/>
					<!--
					<xs:element name="traderBooks" type="FXTraderBooks_Type"/>
					-->
					<xs:element name="traderBooks" type="TraderBooks_Type" minOccurs="0"/>
					<xs:element name="portfolios" type="Portfolios_Type" minOccurs="0"/>
					<xs:element name="nearLegDetails" type="SpotFwdLeg_Type"/>
					<xs:element name="farLegDetails" type="SpotFwdLeg_Type"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SpotFwdAllInRate_Type">
		<xs:sequence>
			<xs:element name="spotRate" type="xs:decimal" minOccurs="0"/>
			<xs:element name="allInRate" type="xs:decimal"/>
			<xs:element name="forwardPoints" type="xs:decimal" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SpreadData_Type">
		<xs:sequence>
			<xs:element name="spot" type="xs:decimal"/>
			<xs:element name="forward" type="xs:decimal"/>
			<xs:element name="allIn" type="xs:decimal"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SpotFwdLeg_Type">
		<xs:sequence>
			<xs:element name="valueDate" type="fxbase:TenorDate_Type"/>
			<xs:element name="wfBuySellIndicator" type="fxbase:BuySellIndicator_Enum"/>
			<xs:element name="currencyAmount" type="wfbase:CurrencyAmt_Type"/>
			<xs:element name="ctrCurrencyAmount" type="wfbase:CurrencyAmt_Type"/>
			<xs:element name="customerRate" type="SpotFwdAllInRate_Type"/>
			<xs:element name="marketRate" type="SpotFwdAllInRate_Type"/>
			<xs:element name="baseRate" type="SpotFwdAllInRate_Type"/>
			<xs:element name="referenceMidRate" type="SpotFwdAllInRate_Type"/>
			<xs:element name="spreadAmount" type="SpreadData_Type"/>
			<xs:element name="spreadRate" type="SpreadData_Type"/>
			<xs:element name="baseCurrencyAmount" type="wfbase:CurrencyAmt_Type"/>
			<xs:element name="currencyTerm" type="fxbase:CurrencyTerm_Enum"/>
			<xs:element name="baseCurrencyTerm" type="fxbase:CurrencyTerm_Enum"/>
			<xs:element name="rateMarketConvention" type="xs:boolean"/>
			<xs:element name="baseRateMarketConvention" type="xs:boolean"/>
			<xs:element name="homeCurrencyMarketRate" type="SpotFwdAllInRate_Type"/>
			<xs:element name="homeCurrencyCustomerRate" type="SpotFwdAllInRate_Type"/>
			<xs:element name="homeCurrencyAmount" type="xs:decimal"/>
			<!--<xs:element name="homeCcyData" type="base:HomeCurrencyData_Type" minOccurs="0"/>-->
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Contract" type="Contract_Type" abstract="true"/>
	<xs:element name="SpotFwdContract" type="SpotFwdContract_Type" substitutionGroup="Contract"/>
	<xs:element name="WindowFwdContract" type="WindowFwdContract_Type" substitutionGroup="Contract"/>
	<xs:element name="SwapContract" type="SwapContract_Type" substitutionGroup="Contract"/>
	<xs:element name="TradeDetailList" type="TradeDetailList_Type" nillable="false">
		<xs:annotation>
			<xs:documentation>A list of TradeDetails</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="TradeDetailList_Type">
		<xs:annotation>
			<xs:documentation>A list of TradeDetails</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="TradeDetail" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="TradeDetail" type="TradeDetail_Type"/>
	<xs:complexType name="TradeDetail_Type">
		<xs:annotation>
			<xs:documentation>Represents a specific trade. Contains and/or Settlement Details</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Contract"/>
			<xs:element name="settlement" type="sc:SettlementData_Type" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
