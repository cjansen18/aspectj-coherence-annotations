	<xs:element name="spotFwd" type="FXSpotFwdTradeRequest_Type"/>
	<xs:complexType name="FXSpotFwdTradeRequest_Type">
		<xs:annotation>
			<xs:documentation>Trade request for booking a FX Spot/Fwd contract</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="TradeRequest_Type">
				<xs:sequence>
					<xs:element name="dealDate" type="xs:date" minOccurs="0"/>
					<xs:element name="valueDate" type="fxbase:TenorDate_Type"/>
					<xs:element name="marketRate" type="SpotFwdAllInRate_Type" minOccurs="0"/>
					<xs:element name="customerRate" type="SpotFwdAllInRate_Type" minOccurs="0"/>
					<xs:element name="baseRate" type="SpotFwdAllInRate_Type" minOccurs="0"/>
					<xs:element name="referenceMidRate" type="SpotFwdAllInRate_Type" minOccurs="0"/>
					<!-- Need to set either option #1 or option #2. Need a better way for this -->
					<!-- Option #1 -->
					<xs:element name="fixedCurrency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
					<xs:element name="wfBuyCurrency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
					<xs:element name="wfBuyCurrencyAmt" type="xs:decimal" minOccurs="0"/>
					<xs:element name="wfSellCurrency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
					<xs:element name="wfSellCurrencyAmt" type="xs:decimal" minOccurs="0"/>
					<!-- End Option #1 -->
					<!-- Option #2 -->
					<xs:element name="currencyTerm" type="fxbase:CurrencyTerm_Enum" minOccurs="0"/>
					<xs:element name="currency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
					<xs:element name="ctrCurrency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
					<xs:element name="currencyAmount" type="xs:decimal" minOccurs="0"/>
					<xs:element name="ctrCurrencyAmount" type="xs:decimal" minOccurs="0"/>
					<xs:element name="wfBuySellIndicator" type="fxbase:BuySellIndicator_Enum" minOccurs="0"/>
					<!-- End Option #2 -->
					<xs:element name="portfolio" type="Portfolios_Type" minOccurs="0"/>
					<xs:element name="traderBook" type="TraderBooks_Type" minOccurs="0"/>
					<!--
					<xs:element name="traderBooks" type="FXTraderBooks_Type" minOccurs="0"/>
					-->
					<xs:element name="dealText" type="xs:string" minOccurs="0"/>
					<xs:element name="homeCurrency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FXWindowFwdTradeRequest_Type">
		<xs:annotation>
			<xs:documentation>Trade Request for booking a FX WindowFwd contract</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="FXSpotFwdTradeRequest_Type">
				<xs:sequence>
					<xs:element name="optionDate" type="fxbase:TenorDate_Type"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FXDrawDownTradeRequest_Type">
		<xs:annotation>
			<xs:documentation>Trade Request for booking a FX DrawDown contract</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="FXSpotFwdTradeRequest_Type"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FXNDFTradeRequest_Type">
		<xs:annotation>
			<xs:documentation>Trade Request for booking a FX NDF (non deliverable forward) contract</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="FXSpotFwdTradeRequest_Type">
				<xs:sequence>
					<xs:element name="fixingDate" type="xs:date" minOccurs="0"/>
