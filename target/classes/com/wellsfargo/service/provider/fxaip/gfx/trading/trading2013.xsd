	<xs:complexType name="TradeRequest_Type" abstract="true">
		<xs:annotation>
			<xs:documentation>Base for all trade requests</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="tradeId" type="xs:integer" minOccurs="0"/>
			<xs:element name="contractId" type="xs:string" minOccurs="0"/>
			<xs:element name="contractSeq" type="xs:string" minOccurs="0"/>
			<xs:element name="branch" type="xs:string"/>
			<xs:element name="customerId" type="fxbase:Entity_Type"/>
			<xs:element name="sourceReferenceId" type="fxbase:ReferenceId_Type"/>
			<xs:element name="sourceSystem" type="fxbase:SourceSystem_Type"/>
			<xs:element name="userId" type="fxbase:Entity_Type" minOccurs="0"/>
			<xs:element name="externalRef1" type="fxbase:ReferenceId_Type" minOccurs="0"/>
			<xs:element name="externalRef2" type="fxbase:ReferenceId_Type" minOccurs="0"/>
			<xs:element name="flipExternalResponse" type="xs:boolean" minOccurs="0"/>
			<xs:element name="salesCredit" type="xs:boolean" minOccurs="0"/>
			<xs:element name="coverTradeId" type="xs:string" minOccurs="0"/>
			<xs:element name="additionalInfo" type="AdditionalInfo_Type" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- This should be exposed in the Service schema -->
	<xs:complexType name="TradeDepositRequest_Type">
		<xs:annotation>
			<xs:documentation>Trade Deposit Request</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="TradeRequest_Type">
				<xs:sequence>
					<xs:element name="backOfficeFields" type="TradeBackOfficeInterfaceFields_Type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="tradeNotes" type="TradeNotes_Type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="revisionDate" type="xs:date"/>
					<xs:element name="lastActivityId" type="xs:long"/>
					<!--End - Sourced from legacy TradeHeader -->
					<!--Begin - Sourced from legacy TradeDeposit -->
					<xs:element name="dealDate" type="xs:date" minOccurs="0"/>
					<xs:element name="valueDate" type="xs:date" minOccurs="0"/>
					<xs:element name="maturityDate" type="xs:date" minOccurs="0"/>
					<xs:element name="rateCode" type="xs:string" minOccurs="0"/>
					<xs:element name="basis" type="xs:string" minOccurs="0"/>
					<xs:element name="tenor" type="xs:string" minOccurs="0"/>
					<xs:element name="depositPlacementIndicator">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="1"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="currency" type="xs:string" minOccurs="0"/>
					<xs:element name="currencyAmount" type="wfbase:CurrencyAmt_Type" minOccurs="0"/>
					<xs:element name="customerInterestRate" type="xs:decimal" minOccurs="0"/>
					<xs:element name="marketInterestRate" type="xs:decimal" minOccurs="0"/>
					<xs:element name="traderBook" type="xs:string" minOccurs="0"/>
					<xs:element name="updateUser" type="xs:string" minOccurs="0"/>
					<xs:element name="updateTime" type="xs:date" minOccurs="0"/>
					<!--End - Sourced from TradeDeposit -->
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
