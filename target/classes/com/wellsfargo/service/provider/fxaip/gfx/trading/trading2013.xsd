					<xs:element name="settlementCurrency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
					<xs:element name="fixingTradeId" type="xs:integer" minOccurs="0"/>
					<xs:element name="fixingDefinition" type="xs:string" minOccurs="0"/>
					<xs:element name="fixingStatus" type="xs:string" minOccurs="0"/>
					<xs:element name="settlementAccount" type="xs:string" minOccurs="0"/>
					<xs:element name="accountType" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FXSwapTradeRequest_Type">
		<xs:annotation>
			<xs:documentation>Trade Request for booking a FX Swap contract</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="TradeRequest_Type">
				<xs:sequence>
					<xs:element name="dealDate" type="xs:date"/>
					<xs:element name="currencyTerm" type="fxbase:CurrencyTerm_Enum" minOccurs="0"/>
					<xs:element name="currency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
					<xs:element name="ctrCurrency" type="wfbase:CurrencyCode_Type" minOccurs="0"/>
					<xs:element name="currencyAmount" type="xs:decimal" minOccurs="0"/>
					<xs:element name="ctrCurrencyAmount" type="xs:decimal" minOccurs="0"/>
					<xs:element name="dealText" type="xs:string" minOccurs="0"/>
					<xs:element name="wfBuySellIndicator" type="fxbase:BuySellIndicator_Enum" minOccurs="0"/>
					<!--
					<xs:element name="traderBooks" type="FXTraderBooks_Type" minOccurs="0"/>
					-->
					<xs:element name="traderBook" type="TraderBooks_Type" minOccurs="0"/>
					<xs:element name="portfolio" type="Portfolios_Type" minOccurs="0"/>
					<xs:element name="nearLeg" type="SpotFwdLeg_Type"/>
					<xs:element name="farLeg" type="SpotFwdLeg_Type"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MCATradeRequest_Type">
		<xs:annotation>
			<xs:documentation>Trade Request for booking a MCA (non-FX) contract</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="TradeRequest_Type">
