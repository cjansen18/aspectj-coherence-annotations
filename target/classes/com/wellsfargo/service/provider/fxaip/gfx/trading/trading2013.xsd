					<xs:element name="deliveryInstructions" type="xs:int" minOccurs="0"/>
					<xs:element name="mcaType" type="McaType_Enum" minOccurs="0"/>
					<xs:element name="specialInstructions" type="xs:string" minOccurs="0"/>
					<xs:element name="valueDate" type="fxbase:TenorDate_Type" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="TradeRequest" type="TradeRequest_Type" abstract="true"/>
	<xs:element name="FXSpotFwdTradeRequest" type="FXSpotFwdTradeRequest_Type" substitutionGroup="TradeRequest"/>
	<xs:element name="FXSwapTradeRequest" type="FXSwapTradeRequest_Type" substitutionGroup="TradeRequest"/>
	<xs:element name="FXNDFTradeRequest" type="FXNDFTradeRequest_Type" substitutionGroup="TradeRequest"/>
	<xs:element name="FXWindowFwdTradeRequest" type="FXWindowFwdTradeRequest_Type" substitutionGroup="TradeRequest"/>
	<xs:element name="FXDrawDownTradeRequest" type="FXDrawDownTradeRequest_Type" substitutionGroup="TradeRequest"/>
	<xs:element name="MCATradeRequest" type="MCATradeRequest_Type" substitutionGroup="TradeRequest"/>
