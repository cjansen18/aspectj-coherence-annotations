<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://service.wellsfargo.com/provider/fxaip/base/2013/"
           xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:base="http://service.wellsfargo.com/base/2003/"
           targetNamespace="http://service.wellsfargo.com/provider/fxaip/base/2013/" elementFormDefault="unqualified"
           attributeFormDefault="unqualified" version="1.01">
	<xs:import namespace="http://service.wellsfargo.com/base/2003/" schemaLocation="../../../base/base2003.xsd"/>
	<!-- -->
	<!-- Copyright 2013 Wells Fargo Bank. All rights reserved.-->
	<!-- 		 										-->
	<!-- BEGIN CHANGE HISTORY: -->
	<!-- 2013-01.03-1.01 Linked to WF XML Base XSD (J.Celauro)-->
	<!-- 2013.01.02-1.00 Initial Version (J.Celauro)-->
	<!-- END CHANGE HISTORY: -->
	<xs:annotation>
		<xs:documentation>Common FXAIP EGI schema definitions will be defined here</xs:documentation>
	</xs:annotation>
	<xs:simpleType name="EntityType_Enum">
		<xs:annotation>
			<xs:documentation>TODO</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="CMR"/>
			<xs:enumeration value="CNO"/>
			<xs:enumeration value="CMNE"/>
			<xs:enumeration value="FXINSIDE"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Entity_Type">
		<xs:annotation>
			<xs:documentation>Represents a Customer (Company) or a User</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="entityType" type="EntityType_Enum" nillable="false"/>
			<xs:element name="entityId" type="xs:string" nillable="false"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SourceSystem_Type">
		<xs:annotation>
			<xs:documentation>Source System the request is originating from</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="sourceSystem" type="xs:string"/>
			<xs:element name="dealSource" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TenorDate_Type">
		<xs:annotation>
			<xs:documentation>Used to represent either ValueDate or OptionDate</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="date" type="xs:date" minOccurs="0"/>
			<xs:element name="tenor" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="ReferenceId_Type">
		<xs:annotation>
			<xs:documentation>Reference data</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="CurrencyPair_Type">
		<xs:annotation>
			<xs:documentation>Represents a currency pair and whether or not it is in market convention</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="numeratorCurrency" type="base:CurrencyCode_Type"/>
			<xs:element name="denominatorCurrency" type="base:CurrencyCode_Type"/>
			<xs:element name="marketConvention" type="xs:boolean"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="PriceDimension_Enum">
		<xs:annotation>
			<xs:documentation>TODO</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="BID"/>
			<xs:enumeration value="ASK"/>
			<xs:enumeration value="BOTH"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TransactionLimitType_Enum">
		<xs:annotation>
			<xs:documentation>Signifies whether the transaction limit check is for the customer or for the user</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="CUSTOMER"/>
			<xs:enumeration value="USER"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BuySellIndicator_Enum">
		<xs:annotation>
			<xs:documentation>From Bank perspective</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="BUY"/>
			<xs:enumeration value="SELL"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CurrencyTerm_Enum">
		<xs:annotation>
			<xs:documentation>TODO</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="D"/>
			<xs:enumeration value="M"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RateType_Enum">
		<xs:annotation>
			<xs:documentation>TODO</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="LIVE"/>
			<xs:enumeration value="RATE_OF_DAY"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PricingType_Enum">
		<xs:annotation>
			<xs:documentation>TODO</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="STATELESS"/>
			<xs:enumeration value="INDICATIVE"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Product_Enum">
		<xs:annotation>
			<xs:documentation>Different Trade (Contract) types available</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="FXDRAWDWN"/>
			<xs:enumeration value="FXFWD"/>
			<xs:enumeration value="FXNDF"/>
			<xs:enumeration value="FXNDFPOSTR"/>
			<xs:enumeration value="FXPOSTRAN"/>
			<xs:enumeration value="FXSPOT"/>
			<xs:enumeration value="FXSWAP"/>
			<xs:enumeration value="FXWINDOFWD"/>
			<xs:enumeration value="MCA"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="TradeContractId_Type">
		<xs:annotation>
			<xs:documentation>A contract (trade) can be identified with either the tradeId or combination of branch+contractId+contractSeq</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="tradeId" type="xs:integer" minOccurs="0"/>
			<xs:element name="branch" type="xs:string" minOccurs="0"/>
			<xs:element name="contractId" type="xs:string" minOccurs="0"/>
			<xs:element name="contractSeq" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="tradeIdentifier" type="TradeContractId_Type"/>
	<xs:complexType name="TradeContractIdList_Type">
		<xs:annotation>
			<xs:documentation>A list of TradeContractId values</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="tradeIdentifier" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
