<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://service.wellsfargo.com/base/2003/" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://service.wellsfargo.com/base/2003/" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="1.12">
