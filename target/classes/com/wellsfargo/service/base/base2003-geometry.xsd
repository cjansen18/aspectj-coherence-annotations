	<!-- BEGIN CHANGE HISTORY:-->
	<!--08/27/2010~R.Damlo changed minInclusive value and annotation for Longitude_Type-->
	<!--10/01/2008~R.Damlo Added appinfo for myNSPrefix and WF2721 -->
	<!--01/17/2007~R.Damlo Added: 'Weight_Type' and 'Length_Type'-->
	<!--06-10-2003~M.Dayton~Initial Revision.  Copied from geometry.xsd-->
	<!-- END CHANGE HISTORY:-->
	<!-- -->
	<xs:annotation>
		<xs:documentation>Fundimental components from geometry.</xs:documentation>
		<xs:appinfo>
			<myNSPrefix>base</myNSPrefix>
		</xs:appinfo>
	</xs:annotation>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--Geographic Coordinates -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:simpleType name="Longitude_Type">
		<xs:annotation>
			<xs:documentation>Angular distance on the earth's surface, measured east or west from the prime meridian at Greenwich, England, to the meridian passing through a position, expressed in decimal degrees.   (08/27/2010) Longitude is given as an angular measurement ranging from 0 degrees at the Prime Meridian to +180 degrees eastward and -180 degrees westward.   Ecliptic longitude is measured from 0 degrees to 360 degrees eastward (the direction that the Sun appears to move relative to the stars).  The values allowed in this component is inclusive of both measurement ranges of Longitude.  An application referencing this type definition must interpret the value to be either an angular or ecliptic measurement and adjust accordingly; such as, a value of -90 may be represented as 270 as an ecliptic longitude.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="-180"/>
			<xs:maxInclusive value="360"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Latitude_Type">
		<xs:annotation>
			<xs:documentation>Angular distance north or south of the earth's equator, measured in decimal degrees along a meridian.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="-90"/>
			<xs:maxInclusive value="+90"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="GeographicPoint_Type">
		<xs:annotation>
			<xs:documentation>A point on the earths's surface given as latitude and longitutde.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="longitude" type="Longitude_Type">
				<xs:annotation>
					<xs:documentation>The angular distance on the earth's surface, measured east or west from the prime meridian at Greenwich, England, to the meridian passing through a position, expressed in decimal degrees </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="latitude" type="Latitude_Type">
				<xs:annotation>
					<xs:documentation>Angular distance north or south of the earth's equator, measured in decimal degrees along a meridian.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GeographicPointList_Type">
		<xs:annotation>
			<xs:documentation>A list of geographic points.</xs:documentation>
			<xs:appinfo>
				<exception>WF2721</exception>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="point" type="GeographicPoint_Type" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A point on the earths's surface given as latitude and longitutde.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--Weight-->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:complexType name="Weight_Type">
		<xs:annotation>
			<xs:documentation>Weight Measurement.  </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="weightValue" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>The Weight amount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="weightUnit" type="xs:string">
				<xs:annotation>
					<xs:documentation>The units of the Weight.  Common units are TON, METRIC_TON, POUND.   Additional information is availalbe at: http://www.unc.edu/~rowlett/units/ </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--Length-->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:complexType name="Length_Type">
		<xs:annotation>
			<xs:documentation>Length Measurement</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="lengthValue" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>The Length amount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lengthUnit" type="xs:string">
				<xs:annotation>
					<xs:documentation>The units of the Length.  Common units are INCH, FOOT, YARD, MILE, METER.   Additional information is availalbe at: http://www.unc.edu/~rowlett/units/ </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- Distance -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:complexType name="Distance_Type">
		<xs:annotation>
			<xs:documentation>A measure of linear distance</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="value" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>The distance measurement, given in distanceUnits</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="distanceUnit" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The units of the linearValue.  If ommited, the units should be understood from the element name.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
