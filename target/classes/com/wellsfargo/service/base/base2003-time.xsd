	<xs:annotation>
		<xs:documentation>Base definitions of components for Time.</xs:documentation>
		<xs:appinfo>
			<myNSPrefix>base</myNSPrefix>
		</xs:appinfo>
	</xs:annotation>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- Ranges -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:complexType name="DateRange_Type">
		<xs:annotation>
			<xs:documentation>Interval between dates. Date range is open ended i.e. either end can be left unspecified.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="startDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>starting date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>end date</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TimeRange_Type">
		<xs:annotation>
			<xs:documentation>The interval between two times. Time range is open ended i.e. either end can be left unspecified.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="startTime" type="xs:time" minOccurs="0">
				<xs:annotation>
					<xs:documentation>starting time</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endTime" type="xs:time" minOccurs="0">
				<xs:annotation>
					<xs:documentation>end time</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TimePeriod_Type">
		<xs:annotation>
			<xs:documentation>A measurement of time duration.  Consider using xs:duration instead.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="count" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>A Measurement of time given in Time Units.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="timeUnit" type="xs:string">
				<xs:annotation>
					<xs:documentation>Units of time such as Seconds, Hours, Minutes, Days, Weeks, Months Years.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DateTimeRange_Type">
		<xs:annotation>
			<xs:documentation>The interval between two dates and times. The range is open ended i.e. either end can be left unspecified.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="startDateTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>starting date and time</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endDateTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>end date and time</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--Days of the Week-->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:simpleType name="DayOfTheWeekCode_Enum">
		<xs:annotation>
			<xs:documentation>An RFC-2822 code for the day of the week.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="SUN">
				<xs:annotation>
					<xs:documentation>Sunday</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MON">
				<xs:annotation>
					<xs:documentation>Monday</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TUE">
				<xs:annotation>
					<xs:documentation>Tuesday</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WED">
				<xs:annotation>
					<xs:documentation>Wednesday</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="THU">
				<xs:annotation>
					<xs:documentation>Thursday</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FRI">
				<xs:annotation>
					<xs:documentation>Friday</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SAT">
				<xs:annotation>
					<xs:documentation>Saturday</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="DayOfTheWeekCodeList_Type">
		<xs:sequence>
			<xs:element name="dayOfTheWeekCode" type="DayOfTheWeekCode_Enum" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Identifies a day of the week</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--Schedule-->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:simpleType name="IntervalCompleteConstraint_Enum">
		<xs:annotation>
			<xs:documentation>Identifies whether the end of an interval is constrained by calendar or banking days.</xs:documentation>
			<xs:appinfo>
				<exception>WF1822</exception>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="LAST_CALENDAR_DAY_MONTH">
				<xs:annotation>
					<xs:documentation>Last calendar day of the month</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LAST_BANK_DAY_MONTH">
				<xs:annotation>
					<xs:documentation>Last bank day of the month</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SPECIFIED_CALENDAR_DAY">
				<xs:annotation>
					<xs:documentation>Specified calendar day</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NEAREST_BANK_DAY_TO_SPECIFIED">
				<xs:annotation>
					<xs:documentation>Nearest bank day to specified day</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="UNKNOWN_CODE">
				<xs:annotation>
					<xs:documentation>Unknown code</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Recurrence_Enum">
		<xs:annotation>
			<xs:documentation>Indicates how frequently( hourly, daily, monthly, annually) a business event takes place.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="HOURLY">
				<xs:annotation>
					<xs:documentation>Hourly</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DAILY">
				<xs:annotation>
					<xs:documentation>Daily</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ANNUALLY">
				<xs:annotation>
					<xs:documentation>Annually</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="QUARTERLY">
				<xs:annotation>
					<xs:documentation>Quarterly</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SEMI_ANNUALLY">
				<xs:annotation>
					<xs:documentation>Semi annually</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WEEKLY">
				<xs:annotation>
					<xs:documentation>Weekly</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BI_WEEKLY">
				<xs:annotation>
					<xs:documentation>Bi-weekly</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MONTHLY_AT_MATURITY">
				<xs:annotation>
					<xs:documentation>Monthly at maturity</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MONTHLY">
				<xs:annotation>
					<xs:documentation>Monthly</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ONE_TIME">
				<xs:annotation>
					<xs:documentation>One time</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="UNKNOWN_CODE">
				<xs:annotation>
					<xs:documentation>Unknown code</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BusinessDateCode_Enum">
		<xs:annotation>
			<xs:documentation>The choices for Business Date Code. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="CURRENT_BUSINESS_DATE">
				<xs:annotation>
					<xs:documentation>Current business date</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NEXT_BUSINESS_DATE">
				<xs:annotation>
					<xs:documentation>Next business date</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CLIENT_DATE">
				<xs:annotation>
					<xs:documentation>Client date</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PREV_BUSINESS_DATE">
				<xs:annotation>
					<xs:documentation>Previous business date</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--Timezone-->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xs:simpleType name="TimeZoneId_Type">
		<xs:annotation>
			<xs:documentation>Identifies the geographical region in which a specific time zone applies (e.g. Arizona). The identifiers are define in Hogan PCD 24182.</xs:documentation>
			<xs:appinfo>
				<exception>WF3181</exception>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="TimeZoneOffset_Type">
		<xs:annotation>
			<xs:documentation>A timezone is identified by the difference between times in that zone and Coordinated Universal Time. It's format is a sign followed by hh:mm e.g. +05:00</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[\-+][0-9][0-9]:[0-9][0-9]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="TimeZone_Type">
		<xs:sequence>
			<xs:element name="offsetFromUTC" type="TimeZoneOffset_Type">
				<xs:annotation>
					<xs:documentation>The difference between times at this location and Coordinated Universal Time in hours and minutes. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="abbreviation" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An abbreviation for the timezone e.g. PDT.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="daylightSavingsIndicator" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates if daylight savings is in in effect.</xs:documentation>
					<xs:appinfo>
						<exception>WF3023</exception>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LocalTime_Type">
		<xs:annotation>
			<xs:documentation>A local time explicitly identifies the timezone outside of the value of an xs:time. This ensures that the timezone information is retained when (un)serialization occurs.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="time" type="xs:time">
				<xs:annotation>
					<xs:documentation>A local time i.e. it does not include a timezone.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="timeZone" type="TimeZone_Type">
				<xs:annotation>
					<xs:documentation>The time zone of the local time</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
	<!--Cycles-->
	<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
	<xs:simpleType name="CycleNumber_Type">
		<xs:annotation>
			<xs:documentation>Indentifies the account cycle</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:maxInclusive value="12"/>
			<xs:minInclusive value="1"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
