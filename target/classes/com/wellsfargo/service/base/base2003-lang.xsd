	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--Languages-->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--The syntax for Language codes is defined by RFC 1766:  http://www.faqs.org/rfcs/rfc1766.html-->
	<!---->
	<xs:annotation>
		<xs:documentation>The syntax for Language codes is defined by RFC 1766:  http://www.faqs.org/rfcs/rfc1766.html </xs:documentation>
		<xs:appinfo>
			<myNSPrefix>base</myNSPrefix>
		</xs:appinfo>
	</xs:annotation>
	<xs:complexType name="LanguageCodeList_Type">
		<xs:annotation>
			<xs:documentation>An list of langauge codes.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="languageCode" type="xs:language" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Language codes are defined by RFC 1766 and ISO-639.  For a list of values, see the 2-character code list  http://www.w3.org/WAI/ER/IG/ert/iso639.htm</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
